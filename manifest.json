{
    "manifest_version": 3,
    "name": "Modérateur",
    "description": "Vérification vocabulaire utilisé",
    "version": "1.0",
    
    "permissions": [
        "activeTab",
        "tabs"
    ],

    // "icons": {
    //     "16": "images/icon16.png",
    //     "48": "images/icon48.png",
    //     "128": "images/icon128.png"
    // },

    "action": {
        "default_incon": "images/icon.jpg",
        "defaul_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                 "<all_urls>",
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/search.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ]
}